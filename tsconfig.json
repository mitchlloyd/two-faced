{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "target": "es5",
    "lib": ["es5"],
    "paths": {
      "*": ["src/*"]
    }
  }
}
